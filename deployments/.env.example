# Base/Shared Environment Variables Example
# Copy this file to .env and update values as needed

# Redis Configuration (Base)
REDIS_IMAGE=redis:7-alpine
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Environment
APP_ENV=development
COMPOSE_PROJECT_NAME=go-shop

# Network Configuration
NETWORK_NAME=go-shop-network

# Volume Configuration
REDIS_VOLUME_NAME=redis_data
